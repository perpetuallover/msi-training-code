package com.kenny.test;

public class SwapInClass {
	public void swap(int a, int b){
		a = a + b;
		b = a - b;
		a = a - b;
		System.out.println(a);
		System.out.println(b);
	}
	public static void main(String[] args){
		SwapInClass sic = new SwapInClass();
		int a = 1;
		int b = 99;
		sic.swap(a, b);
		System.out.println(a);
		System.out.println(b);
	}
}
